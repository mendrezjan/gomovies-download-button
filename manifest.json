{
    "manifest_version": 2,

    "name": "gomovies download button",
    "description": "Add a download button to 123movies/gomovies.",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png"
    },
    "permissions": [
        "tabs", "https://gomovies.to/"
    ],
    "background": {
        "scripts": ["background.js"],
        "persistent": true
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"}

}
